{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/_expo/static/js/web/(.*\\.js)$",
            "headers": {
                "Content-Type": "application/javascript",
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        {
            "src": "/_expo/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        {
            "src": "/(.*\\.(js|json|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))$",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}