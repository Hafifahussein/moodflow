{
    "cleanUrls": false,
    "trailingSlash": false,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": null,
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/static/js/(.*\\.js)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript; charset=utf-8"
                }
            ]
        }
    ]
}